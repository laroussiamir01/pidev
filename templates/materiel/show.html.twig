{% extends 'base-back.html.twig' %}

{% block title %}Materiel{% endblock %}

{% block body %}
    <h1>Materiel</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ materiel.id }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ materiel.libelle }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ materiel.type }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ materiel.prix }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_materiel_index') }}">Retour a la liste  |</a>

    <a href="{{ path('app_materiel_edit', {'id': materiel.id}) }}">Mise a jour</a>

    {{ include('materiel/_delete_form.html.twig') }}
{% endblock %}
